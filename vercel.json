{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.py"
    }
  ],
  "env": {
    "VERCEL": "1"
  },
  "functions": {
    "app.py": {
      "memory": 1024,
      "maxDuration": 10,
      "maxLambdaSize": "15mb"
    }
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}